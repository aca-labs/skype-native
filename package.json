{
  "name": "skype-native",
  "version": "0.3.1",
  "description": "Node bindings for the native Skype for Business SDK",
  "repository": "acaprojects/skype-native",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "skype",
    "lync",
    "windows",
    "native"
  ],
  "author": "Kim Burgess <kim@acaprojects.com>",
  "license": "MIT",
  "scripts": {
    "prepublish": "gulp build",
    "pretest": "gulp lint",
    "test": "nyc mocha --opts mocha.opts"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov"
    ]
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-msbuild": "^0.4.7",
    "gulp-run": "^1.7.1",
    "gulp-tslint": "^8.0.0",
    "gulp-typedoc": "^2.0.2",
    "gulp-typescript": "^3.1.6",
    "gulp-util": "^3.0.8",
    "merge2": "^1.0.3",
    "mocha": "^3.4.1",
    "nyc": "^10.3.2",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "edge": "^7.10.0",
    "electron-edge": "^6.5.5"
  }
}
