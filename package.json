{
  "name": "skype-native",
  "version": "0.10.1",
  "description": "Node bindings for the native Skype for Business SDK",
  "repository": "acaprojects/skype-native",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "skype",
    "lync",
    "windows",
    "native"
  ],
  "author": "Kim Burgess <kim@acaprojects.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist ./docs ./coverage ./.nyc_output ./.vs",
    "lint": "tslint -p . --type-check",
    "compile:typescript": "tsc",
    "compile:native": "gulp compile:native",
    "doc": "typedoc --out docs src --name skype-native --readme README.md --excludeNotExported --includeDeclarations --target ES6",
    "build": "npm-run-all clean -p compile:** -s doc",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.4",
    "@types/gulp-msbuild": "^0.5.0",
    "@types/node": "^8.0.34",
    "@types/ramda": "0.24.16",
    "@types/request": "2.0.4",
    "gulp": "^3.9.1",
    "gulp-message": "^1.0.4",
    "gulp-msbuild": "^0.5.4",
    "npm-run-all": "^4.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "edge-ts": "^1.1.1",
    "ramda": "^0.25.0",
    "request": "^2.83.0"
  }
}
